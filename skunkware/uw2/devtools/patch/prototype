i admin=admin
i pkginfo=pkginfo

d patch usr 0755 root sys
d patch usr/local 0755 root sys
d patch usr/local/bin 0755 root sys
s patch usr/local/bin/gpatch=patch
f patch usr/local/bin/patch 0755 root sys
d patch usr/local/man 0755 root sys
d patch usr/local/man/man1 0755 root sys
f patch usr/local/man/man1/patch.1 0644 root sys
d patch usr/local/man/cat1 0755 root sys
f patch usr/local/man/cat1/patch.1 0644 root sys
