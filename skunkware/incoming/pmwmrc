	"hiramc"	f.exec "xterm -tn ansi -fn 10x20 -g 80x25+50+50 -fg white -bg 'dark olive green' -n hiramc -T hiramc -sb -e rlogin johndoe  -l hiramc &"
	"shawnm"		f.exec "xterm -tn ansi -fn 10x20 -g 80x25+50+50 -fg yellow -bg maroon -n shawnm -T shawnm -sb -e rlogin johndoe  -l shawnm &"
