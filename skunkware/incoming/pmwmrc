	"hyperion"	f.exec "xterm -tn ansi -fn 10x20 -g 80x25+50+50 -fg yellow -bg maroon -n hyperion -T hyperion -sb -e telnet hyperion &"
	"crom"	f.exec "xterm -tn ansi -fn 10x20 -g 80x25+50+50 -fg yellow -bg maroon -n crom -T crom -sb -e telnet crom &"
	"disco"	f.exec "xterm -tn ansi -fn 10x20 -g 80x25+50+50 -fg cyan -bg 'midnight blue' -cr yellow -n disco -T disco -sb -e telnet disco &"
	"stage"	f.exec "xterm -tn ansi -fn 10x20 -g 80x25+50+50 -fg yellow -bg blue -n stage -T stage -sb -e telnet -l rjr stage &"
	"fourmile"	f.exec "xterm -tn ansi -fn 10x20 -g 80x25+50+50 -fg yellow -bg blue -n fourmile -T fourmile -sb -e rlogin fourmile &"
	"bugsco"	f.exec "xterm -tn ansi -fn 10x20 -g 80x25+50+50 -fg yellow -bg black -n bugsco -T bugsco -sb -e telnet bugsco &"
	"novell"	f.exec "xterm -tn ansi -fn 10x20 -g 80x25+50+50 -fg cyan -bg 'midnight blue' -n novell -T novell -sb -e telnet charon.fpk.novell.com &"
	"other"	f.exec "xterm -tn ansi -fn 10x20 -g 80x25+50+50 -fg yellow -bg blue -n other -T other -sb -e telnet &"
}

