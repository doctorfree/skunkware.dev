# Root Menu Description
Menu RootMenu
{
    "New Window"		f.exec "Eterm -F 10x20 &"
#    "New Window"		f.exec "xterm -ls -sb -tn ansi -fn 10x20 -bg 'midnight blue' -cr yellow -fg cyan -g 80x25+50+50 -title \"rr\" &"
    "Login"    			f.menu LoginMenu
    "Telnet"			f.menu TelnetMenu
	"root"	f.exec "xterm -tn ansi -fn 10x20 -fg cyan -bg 'midnight blue' -cr yellow -n root -T root -sb -e rlogin johndoe  -l root &"
	"plato"	f.exec "xterm -tn ansi -fn 10x20 -g 80x25+50+50 -fg cyan -bg 'midnight blue' -cr yellow -n plato -T plato -sb -e telnet plato &"
    no-op		f.separator
    "Help"              f.exec "scohelp"
    "Netscape"		f.exec "/usr/bin/netscape"
    "XChat"          f.exec "/opt/gnome/bin/xchat"
    "DOS Tools"          f.exec "/usr/local/bin/MToolsFM"
    no-op		f.separator
    "Screen Lock" 	f.exec "xlock > /tmp/xlock.out 2>&1"
    "Root Window Decoration"	f.menu BackMenu
    "Clear Root Window"	f.exec "xsetroot -solid black"
    "Applications"    			f.menu AppMenu
    "Images"    			f.menu ImageMenu
    "Games"    			f.menu GamesMenu
    no-op		f.separator
	"Panner/Icon Menu"	f.menu "PanMenu"
    "Refresh"		f.refresh
    "Preferences"	f.exec "/usr/bin/X11//pconfig &"
    "Restart"	        f.restart
    "Exit Session"      f.quit_mwm
}

