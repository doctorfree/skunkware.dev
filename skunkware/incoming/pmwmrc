# Login Menu

Menu LoginMenu
{
	"root"	f.exec "xterm -tn ansi -fn 10x20 -g 80x25+50+50 -fg white -bg 'violet red' -n root -T root -sb -e rlogin johndoe  -l root &"
	"sccsadm"	f.exec "xterm -tn ansi -fn 10x20 -g 80x25+50+50 -fg cyan -bg 'dark violet' -n sccsadm -T sccsadm -sb -e rlogin johndoe  -l sccsadm &"
	"rr"	f.exec "xterm -tn ansi -fn 10x20 -g 80x25+50+50 -cr magenta -bd green -fg cyan -bg blue -n rr -T rr -sb &"
	"demo"	f.exec "xterm -tn ansi -fn 10x20 -g 80x25+50+50 -fg cyan -bg 'dark violet' -n demo -T demo -sb -e rlogin johndoe  -l demo "
	"kdemo"	f.exec "xterm -tn ansi -fn 10x20 -g 80x25+50+50 -fg cyan -bg 'dark violet' -n kdemo -T kdemo -sb -e rlogin johndoe  -l kdemo &"
