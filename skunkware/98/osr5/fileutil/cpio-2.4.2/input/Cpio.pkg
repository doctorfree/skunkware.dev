
PKG:Cpio:
description        = "GNU Cpio"
dependencies       = 
distTreeRootSHARED = $Cpio_DIR
distTreeRootCLIENT = $Cpio_DIR

DIR:Cpio:SHARED:usr:
mode  = 0755
owner = root
group = sys
flags = 

DIR:Cpio:SHARED:usr/local:
mode  = 0755
owner = root
group = sys
flags = 

DIR:Cpio:SHARED:usr/local/bin:
mode  = 0755
owner = root
group = sys
flags = 

FILE:Cpio:SHARED:usr/local/bin/cpio:
mode  = 0755
owner = root
group = sys
flags = 
