
PKG:Xlock:
description        = "Xlockmore - Locks the local X display"
dependencies       = SKUNK98:Glib::1.3 
distTreeRootSHARED = $Xlock_DIR
distTreeRootCLIENT = $Xlock_DIR

DIR:Xlock:SHARED:usr:
mode  = 0755
owner = root
group = auth
flags = 

DIR:Xlock:SHARED:usr/local:
mode  = 0755
owner = root
group = auth
flags = 

