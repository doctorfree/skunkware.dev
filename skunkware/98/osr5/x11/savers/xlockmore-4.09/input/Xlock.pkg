mode  = 0755
owner = root
group = auth
flags = 

FILE:Xlock:SHARED:usr/local/bin/xlock:
mode  = 2755
owner = root
group = auth
flags = 
exportPath = /usr/local/bin/xlock

FILE:Xlock:SHARED:usr/local/bin/xmlock:
