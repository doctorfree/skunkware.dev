
PKG:XHTML:
description        = "XHTML Editor"
dependencies       = 
distTreeRootSHARED = $XHTML_DIR
distTreeRootCLIENT = $XHTML_DIR

DIR:XHTML:SHARED:./usr:
mode  = 0755
owner = bin
group = bin
flags = 

DIR:XHTML:SHARED:./usr/lib:
mode  = 0755
owner = bin
group = bin
flags = 

DIR:XHTML:SHARED:./usr/lib/xhtml:
mode  = 0755
owner = bin
group = bin
flags = 
exportPath = /usr/lib/xhtml

FILE:XHTML:SHARED:./usr/lib/xhtml/help.html:
mode  = 0644
owner = bin
group = bin
flags = 

