DIR:XHTML:SHARED:./usr/local/man/cat.1:
mode  = 0755
owner = bin
group = bin
flags = 

FILE:XHTML:SHARED:./usr/local/man/cat.1/xhtml.1:
mode  = 0644
owner = bin
group = bin
flags = 
exportPath = /usr/local/man/cat.1/xhtml.1
