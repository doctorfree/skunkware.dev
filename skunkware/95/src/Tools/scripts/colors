case `tty` in
	/dev/console)
		setcolor hi_white blue
		setcolor -g yellow green
		setcolor -r yellow lt_magenta
		setcolor -o blue
		setcolor -c 10 12 
		setcolor -p 3000 1
		;;
	/dev/tty01)
		setcolor hi_white blue
		setcolor -g yellow green
		setcolor -r yellow lt_magenta
		setcolor -o blue
		setcolor -c 10 12 
		setcolor -p 3000 1
		;;
	/dev/tty02)
		setcolor lt_cyan blue
		setcolor -o blue
		setcolor -g yellow green
		setcolor -r yellow lt_magenta
		setcolor -c 10 12 
		setcolor -p 2900 1
		;;
	/dev/tty03)
		setcolor lt_magenta blue
		setcolor -o blue
		setcolor -g yellow red
		setcolor -r yellow lt_magenta
		setcolor -c 10 12 
		setcolor -p 2800 1
		;;
	/dev/tty04)
		setcolor lt_green blue
		setcolor -o blue
		setcolor -g yellow green
		setcolor -r yellow lt_magenta
		setcolor -c 10 12 
		setcolor -p 2700 1
		;;
	/dev/tty05)
		setcolor lt_cyan blue
		setcolor -o blue
		setcolor -g hi_white blue
		setcolor -r red black
		setcolor -c 10 12 
		setcolor -p 2600 1
		;;
	/dev/tty06)
		setcolor yellow blue
		setcolor -o blue
		setcolor -g lt_green blue
		setcolor -r blue yellow
		setcolor -c 10 12 
		setcolor -p 2300 1
		;;
	/dev/tty07)
		setcolor lt_cyan gray
		setcolor -o gray
		setcolor -g yellow green
		setcolor -r yellow lt_magenta
		setcolor -c 10 12 
		setcolor -p 2400 1
		;;
	/dev/tty08)
		setcolor lt_cyan magenta
		setcolor -o magenta
		setcolor -g yellow green
		setcolor -r yellow lt_magenta
		setcolor -c 10 12 
		setcolor -p 2500 1
		;;
	/dev/tty09)
		setcolor yellow magenta
		setcolor -o magenta
		setcolor -g yellow green
		setcolor -r yellow lt_magenta
		setcolor -c 10 12 
		setcolor -p 2200 1
		;;
	/dev/tty10)
		setcolor magenta black
		setcolor -o black
		setcolor -g yellow green
		setcolor -r yellow lt_magenta
		setcolor -c 10 12 
		setcolor -p 2100 1
		;;
	/dev/tty11)
		setcolor yellow brown
		setcolor -o brown
		setcolor -g yellow green
		setcolor -r yellow lt_magenta
		setcolor -c 10 12 
		setcolor -p 2000 1
		;;
	/dev/tty12)
		setcolor lt_magenta blue
		setcolor -o blue
		setcolor -g yellow green
		setcolor -r blue lt_magenta
		setcolor -c 10 12 
		setcolor -p 1900 1
		;;
	*)
		:
		;;
esac
