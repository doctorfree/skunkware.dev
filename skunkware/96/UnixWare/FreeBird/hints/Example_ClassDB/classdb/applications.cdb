#
#	Rules for locally installed applications
#

#
#	Seyon Communications program
#
CLASS SEYON
BEGIN
	#SYMLINK	"Accessories/Seyon";
	_LPATTERN	"seyon";
	_FILETYPE	EXEC;
	_ICONFILE	"/usr/local/X/lib/pixmaps/terminl.xpm";
	MENU _Open	"exec %F &";
END

#
#	Elm electronic mailer
#
CLASS ELM
BEGIN
	#SYMLINK	"Accessories/Elm Mailer";
	_LPATTERN	"elm";
	_FILETYPE	EXEC;
	_ICONFILE	"/usr/local/X/lib/pixmaps/email2.xpm";
	MENU _Open	"exec %f &";
	_DROP		"exec %f -i'%S' '' &";
END
