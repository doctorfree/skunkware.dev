CLASS rlogin
BEGIN
        _ICONFILE       'netnode.icon';
        _DFLTICONFILE   'exec.icon';
        _PATTERN        'rlogin*';
        _FILETYPE       'DATA';
        MENU _Open      'exec xterm -E rlogin `basename %F | cut -c8-72` &';
        MENU windsor    'exec xterm -E rlogin windsor';
        MENU marlow     'exec xterm -E rlogin marlow';
        MENU lechlade   'exec xterm -E rlogin lechlade';
        MENU uel        'exec xterm -E rlogin uel';
END
